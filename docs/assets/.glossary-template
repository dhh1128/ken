    <script>
var getAbsoluteUrl = (function() {
  var a;
  return function(url) {
      if (!a)
        a = document.createElement('a');
      a.href = url;
      return a.href;
  };
})();

function copyUrl(url) {
  x = getAbsoluteUrl(url);
  navigator.clipboard.writeText(x);
}

function goto(url) {
  window.location = url;
}
